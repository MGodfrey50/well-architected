{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml",
          "**/toc.yml",
          "**/index.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.css",
          "**/*.png",
          "**/*.jpg",
          "**/*.jpeg",
          "**/*.gif",
          "**/*.svg",
          "**/*.js",
          "**/*.json",
          "**/*.docx",
          "feed.atom"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/azure/well-architected/breadcrumb/toc.json",
      "extendBreadcrumb": false,
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/well-architected",
      "brand": "azure",
      "uhfHeaderId": "azure",
      "titleSuffix": "Microsoft Azure Well-Architected Framework",
      "ms.service": "waf",
      "searchScope": [ "Azure", "Microsoft Azure Well-Architected Framework" ],    
     },
    "open_to_public_contributors": {
      "**/*.yml": false
    },
    "fileMetadata": {
      "ms.subservice": {
        "reliability/**/*.md": "waf-pillar-reliability",
        "security/**/*.md": "waf-pillar-security",
        "cost-optimization/**/*.md": "waf-pillar-optimization",
        "performance-efficiency/**/*.md": "waf-pillar-efficiency",
        "operational-excellence/**/*.md": "waf-pillar-excellence",
        "mission-critical/**/*.md": "waf-workload-mission-critical",
        "carrier-grade/**/*.md": "waf-workload-carrier-grade",
        "hybrid/**/*.md": "waf-workload-hybrid",
        "iot/**/*.md": "waf-workload-iot",
        "sap/**/*.md": "waf-workload-sap",
        "sustainability/**/*.md": "waf-workload-sustainability",
        "services/**/*.md": "waf-service-guide"
      }
    },
    "template": [
      "docs.html"
    ],
    "dest": "well-architected",
    "markdownEngineName": "markdig"
  }
}
